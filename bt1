#include <stdio.h>

int main(){
	
	int a[] = { 10 , 20 , 30 , 40 , 50 };
	int do_dai = sizeof( a ) / sizeof(a[0]);
	int i = 0; 

	
	printf("Cac phan tu trong mang la : \n");
	for( i = 0 ; i < do_dai ; i++){
		printf("Phan tu thu %d : %d \n" , i , a[i] ); 
	}
	
	printf("\nDo dai so luong phan tu cua mang la : %d\n " , do_dai );
	
	
	return 0; 
}
