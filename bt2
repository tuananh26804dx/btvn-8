#include <stdio.h>

int main(){
	
int a[5];
int i;
int size = 5; 


for( i = 0 ; i < size ; i++ ){
	
	printf("Hay nhap tu thu %d vao day : " , i);
	scanf("%d" , &a[i]); 
	
} 

for ( i = 0 ; i < size ; i++ ){
	
	printf("Phan tu thu %d vi tri %d : %d \n " , i + 1 , i , a[i]); 
} 

for ( i = 0 ; i < size ; i++ ){
	printf("%d" , a[i]); 
}


return 0 ; 
} 
